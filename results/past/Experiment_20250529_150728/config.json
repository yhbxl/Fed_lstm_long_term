{
    "experiment_name": "lstm_federated_learning",
    "output_dir": "results",
    "dataset": "mnist",
    "num_classes": 10,
    "batch_size": 32,
    "num_clients": 10,
    "num_rounds": 500,
    "selection_fraction": 0.1,
    "alpha": 0.5,
    "model": "cnn",
    "lr": 0.01,
    "momentum": 0.9,
    "weight_decay": 0.0001,
    "lstm_input_size": 1,
    "lstm_hidden_size": 128,
    "lstm_num_layers": 2,
    "lstm_dropout": 0.2,
    "lstm_learning_rate": 0.001,
    "lstm_sequence_length": 5,
    "lstm_patience": 5,
    "window_size": 5,
    "min_window_size": 3,
    "max_window_size": 10,
    "window_adjust_threshold": 0.01,
    "performance_weight": 0.7,
    "delay_weight": 0.3,
    "eval_interval": 1,
    "seed": 42
}